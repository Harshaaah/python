numbers=[-10,15,-3,7,25,18,0]
positive=[num for num in numbers if num>0]
print(f"positive numbers in {numbers} : {positive}")
print()
n=5
squares=[num**2 for num in range(1,n+1)]
print("squares of first 5 numbers:",squares)
print()
word="comprehension"
vowels=[char for char in word if char in 'aeiou']
print(f"vowels in word:{word}",vowels)
print()
word="hello"
oridinal=[ord(char) for char in word]
print("oridinal values of each character in the word:hello",oridinal)
