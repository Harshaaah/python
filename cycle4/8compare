def compare(s1,s2,n):
        if s1[:n]==s2[:n]:
                return True
        else:
                return False
s1=input("enter 1st string:")
s2=input("enter 2nd string:")
n=int(input("enter no. of characters to compare:"))
result=compare(s1,s2,n)
if result:
        print("the 1st ",n,"characters of both string are same")
else:
        print("the 1st ",n," charactrs are not same")
